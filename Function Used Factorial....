#include<stdio.h>
int factorial(int n)
{
    if(n==0 ||n==1)
    return 1;

else

return n* factorial(n-1);
}
int main()
{
    int num;
    printf("Enter a Positive Number :");
    scanf("%d",&num);
    if(num<0)
    {
        printf("Value is Not Accessable :%d",num);
    }
    else
    {
        int result=factorial(num);
        printf("Factorial Of %d = %d\n",num,result);
        
    }
    return 0;
    
}
